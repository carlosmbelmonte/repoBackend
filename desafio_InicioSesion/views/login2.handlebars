<div id="loginPrevio" class="mx-3 mb-3 mt-3 rounded-4 bg-secondary bg-opacity-10 p-2">
    <h1 class="text-primary display-5 fw-bold mx-3">Login de Usuario</h1>
    <form>
        <div class="form-group mx-3">
            <label class="form-label fst-italic fw-bold">Ingrese su Mail y Contraseña</label>
            <input type="text" id="inputLogInMail" name='inputLogInMail' class="form-control mb-1" placeholder="Mail"> 
            <input type="password" id="inputLogInPassword" name='inputLogInPassword' class="form-control" placeholder="Contraseña">
            <p class="mx-3 fw-bold text-danger mb-0" id="mensajeErrorLogIn"></p>
        </div>
        <div class="form-group">
            <button class="btn btn-success mx-3 mt-3 mb-3" id="enviarLogIn">Login</button>
        </div>
    </form>
    <button class="btn btn-success mx-3 mb-3" id="gotoLogin">Ir a Register</button>
</div>